	<parameter name="idClub" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     cast(avg(reuniones."reu_valoracion") as decimal (3,2)) AS reuniones_reu_valoracion,
     reuniones."isbn" AS reuniones_isbn,
     INITCAP(libro."lib_tit_original") AS libro_lib_tit_original
FROM
     "public"."grupo" grupo INNER JOIN "public"."reuniones" reuniones ON grupo."club_id" = reuniones."club_id"
     AND grupo."grup_id" = reuniones."grup_id"
     INNER JOIN "public"."club" club ON grupo."club_id" = club."club_id"
     INNER JOIN "public"."g_lector" g_lector ON grupo."club_id" = g_lector."club_id"
     AND grupo."grup_id" = g_lector."grup_id"
     AND g_lector."club_id" = reuniones."clubg_l_id"
     AND g_lector."doc_id" = reuniones."doc_id"
     AND g_lector."fechai_mie" = reuniones."fechai_mie"
     AND g_lector."clubh_id" = reuniones."clubh_id"
     AND g_lector."grup_id" = reuniones."grupg_l_id"
     INNER JOIN "public"."hist_miembro" hist_miembro ON g_lector."doc_id" = hist_miembro."doc_id"
     AND hist_miembro."club_id" = g_lector."clubh_id"
     AND hist_miembro."fechai_mie" = g_lector."fechai_mie"
     AND club."club_id" = hist_miembro."club_id"
     INNER JOIN "public"."miembro" miembro ON hist_miembro."doc_id" = miembro."doc_id"
     INNER JOIN "public"."libro" libro ON reuniones."isbn" = libro."isbn"
where reuniones.club_id = $P!{idClub} and reuniones.clubg_l_Id =$P!{idClub} 
     and reuniones.clubh_id =$P!{idClub}
GROUP BY reuniones.isbn , libro.lib_tit_original]]>
	</queryString>
