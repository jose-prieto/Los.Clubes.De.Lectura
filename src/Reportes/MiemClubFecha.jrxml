	<queryString>
		<![CDATA[select TO_CHAR(NOW() :: DATE, 'DD "de" TMMonth "del" yyyy') as fecha_hoy,

concat(initcap(mi.miemb_ape1), '  ', COALESCE (initcap(mi.miemb_ape2), '')) as apellidos,

concat(initcap(mi.miemb_nombre1), '  ', COALESCE (initcap(mi.miemb_nombre2), '')) as nombres,

mi.doc_id as cedula,

mi.miemb_genero as genero,

TO_CHAR(mi.miemb_fecha_nac :: DATE, 'DD "de" TMMonth "del" yyyy') as fecha_nacimiento,

(select concat(extract(year from age(mi.miemb_fecha_nac)),' años')
	from miembro mie
		where mie.doc_id = mi.doc_id) as edad,

COALESCE ((select concat(COALESCE (initcap(rep_ape1),'---------- ----------'), '  ',COALESCE (initcap(rep_ape2),''))
	from representante rep
		where rep.doc_ident = mi.doc_id),'---------- ----------') as apellido_rep,

COALESCE ((select concat(COALESCE (initcap(rep_nombre1),'---------- ----------'), '  ',COALESCE (initcap(rep_nombre2),''))
	from representante rep
		where rep.doc_ident = mi.doc_id),'---------- ----------') as nombre_rep,

$P!{fecha} as fecha

from miembro mi, reuniones reu

where mi.doc_id = $P!{id}
]]>
	</queryString>
