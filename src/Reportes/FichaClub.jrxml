	<parameter name="idClub" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     (select INITCAP(idioma.idio_nombre) from idioma 
                      where idio_id = club.idio_id) AS idioma_idio_nombre,

     INITCAP(club.club_nombre) AS club_club_nombre,

     INITCAP(club.direccion) AS club_direccion,

     (select INITCAP(direccion_lugar.dir_nombre) from direccion_lugar
                      where dir_id =club.dir_id) AS direccion_lugar_dir_nombre,

     (select INITCAP(direccion_lugar.dir_nombre) from direccion_lugar
                      where dir_id =( select direccion_lugar.dir_id_padre from direccion_lugar
                                      where dir_id=club.dir_id )) AS direccion_lugar_A_dir_nombre,
     club.club_fecha_inicio AS club_club_fecha_inicio,

     club.cod_postal AS club_cod_postal,

     club.cuota AS club_cuota,

     club.inst_id AS club_inst_id,

     grupo.grup_id AS grupo_grup_id,

     grupo.grup_tipo AS grupo_grup_tipo,

     grupo.dia AS grupo_dia,

     grupo.horai AS grupo_horai


from club, grupo
where club.club_id=$P!{idClub} and grupo.club_id=$P!{idClub}]]>
	</queryString>
