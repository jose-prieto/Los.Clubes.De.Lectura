	<subDataset name="Inasistencia" uuid="8f3b5675-fec4-4e17-a135-7033954ba035">
		<parameter name="grup_id" class="java.lang.String"/>
		<parameter name="reu_fecha" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
     initcap(miembro."miemb_nombre1") AS miembro_miemb_nombre1,
     initcap(miembro."miemb_ape1") AS miembro_miemb_ape1
FROM
     "public"."miembro" miembro INNER JOIN "public"."hist_miembro" hist_miembro ON miembro."doc_id" = hist_miembro."doc_id"
     INNER JOIN "public"."g_lector" g_lector ON hist_miembro."doc_id" = g_lector."doc_id"
     AND hist_miembro."club_id" = g_lector."clubh_id"
     AND hist_miembro."fechai_mie" = g_lector."fechai_mie"
     INNER JOIN "public"."inasistencia" inasistencia ON g_lector."club_id" = inasistencia."club_id"
     AND g_lector."fechai_mie" = inasistencia."fechai_mie"
     AND g_lector."grup_id" = inasistencia."grup_id"
     AND g_lector."clubh_id" = inasistencia."clubh_id"
     AND g_lector."doc_id" = inasistencia."doc_id"
WHERE
     inasistencia.grupr_id = $P!{grup_id} AND inasistencia.reu_fecha = to_date($P{reu_fecha}, 'yyyy-mm-dd')]]>
		</queryString>
		<field name="miembro_miemb_nombre1" class="java.lang.String"/>
		<field name="miembro_miemb_ape1" class="java.lang.String"/>
		<group name="miembro_miemb_nombre1">
			<groupExpression><![CDATA[$F{miembro_miemb_nombre1}]]></groupExpression>
		</group>
		<group name="miembro_miemb_ape1">
			<groupExpression><![CDATA[$F{miembro_miemb_ape1}]]></groupExpression>
		</group>
	</subDataset>
	<parameter name="grup_id" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="reu_fecha" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     COALESCE(reuniones."reu_conclusiones",'No hay conclusiones') AS reuniones_reu_conclusiones,
     COALESCE(to_char(reuniones."reu_valoracion",''),'No hay valoraciones') AS reuniones_reu_valoracion,
     initcap(club."club_nombre") AS club_club_nombre,
     initcap(libro."lib_tit_original") AS libro_lib_tit_original,
     concat(initcap(miembro."miemb_nombre1"),' ',initcap(miembro."miemb_ape1")) AS nombrecompleto,
     reuniones."reu_fecha" AS reuniones_reu_fecha,
     reuniones."grup_id" AS reuniones_grup_id,
     grupo."horai" AS grupo_horai,
     grupo."horaf" AS grupo_horaf
FROM
     "public"."hist_miembro" hist_miembro INNER JOIN "public"."g_lector" g_lector ON hist_miembro."doc_id" = g_lector."doc_id"
     AND hist_miembro."club_id" = g_lector."clubh_id"
     AND hist_miembro."fechai_mie" = g_lector."fechai_mie"
     INNER JOIN "public"."reuniones" reuniones ON g_lector."club_id" = reuniones."clubg_l_id"
     AND g_lector."grup_id" = reuniones."grupg_l_id"
     AND g_lector."doc_id" = reuniones."doc_id"
     AND g_lector."clubh_id" = reuniones."clubh_id"
     AND g_lector."fechai_mie" = reuniones."fechai_mie"
     INNER JOIN "public"."grupo" grupo ON g_lector."club_id" = grupo."club_id"
     AND grupo."grup_id" = g_lector."grup_id"
     AND grupo."club_id" = reuniones."club_id"
     AND grupo."grup_id" = reuniones."grup_id"
     INNER JOIN "public"."club" club ON grupo."club_id" = club."club_id"
     AND club."club_id" = hist_miembro."club_id"
     INNER JOIN "public"."miembro" miembro ON hist_miembro."doc_id" = miembro."doc_id"
     INNER JOIN "public"."libro" libro ON reuniones."isbn" = libro."isbn"
WHERE
     reuniones.grup_id = $P!{grup_id}
 AND reuniones.reu_fecha = to_date($P{reu_fecha},'yyyy-mm-dd')]]>
	</queryString>
